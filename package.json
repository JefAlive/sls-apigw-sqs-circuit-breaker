  "main": "index.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint .",
    "setup": "npm i && sls deploy",
    "auth": "bash ./scripts/auth.bash",
    "format": "prettier --write .",
    "getJWT": "npm run auth dev us-east-1 signup && npm run auth dev us-east-1 signin",
